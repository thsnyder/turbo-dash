{
  "manifest_version": 3,
  "name": "Turbo Dash",
  "version": "1.0",
  "description": "A turbocharged dashboard for your new tab",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "permissions": [
    "geolocation"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
