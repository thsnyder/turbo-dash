{
  "manifest_version": 3,
  "name": "Turbo Dash",
  "version": "1.5",
  "description": "A turbocharged dashboard for your new tab",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "permissions": [],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [{
    "resources": ["data/roadmap.json", "data/quotes.json"],
    "matches": ["<all_urls>"]
  }]
}
